{{>layout/header}}
    <h1>tuioe</h1>
    <div class="col-md-12">
        <div class="row">
            <div class="col-md-12">
                <a href="/products/save" role="button" class="btn btn-primary">글 등록</a>
            </div>
        </div>
        <br>
        <table class="table table-horizontal table-bordered">
            <thread class="thread-strong">
                <tr>
                    <th>상품번호</th>
                    <th>상품이름</th>
                    <th>상품가격</th>
                </tr>
            </thread>
            <tbody id="tbody">
            {{#product}}
                <tr>
                    <td>{{p_id}}</td>
                    <td><a href="/products/update/{{p_id}}">{{p_name}}</a></td>
                    <td>{{p_price}}</td>
                </tr>
            {{/product}}
            </tbody>
        </table>
    </div>
{{>layout/footer}}
